<Window x:Name="frmBarbezDotEu.Executor" x:Class="HanTools.BarbezDotEu.Executor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Multilink Browser Opener" Height="300" Width="300">
    <Grid>
        <Button x:Name="btnOpenNextLinkInBrowser" Content="Open Next Link In Browser" Margin="127,0,10,25" VerticalAlignment="Bottom" Height="30" Click="btnOpenNextLinkInBrowser_Click"/>
        <Button x:Name="btnReset" Content="Reset" Margin="10,0,0,25" VerticalAlignment="Bottom" Height="30" HorizontalAlignment="Left" Width="112" Click="btnReset_Click"/>
        <TextBox x:Name="tbInput" AcceptsReturn="True" Margin="10,10,10,60" TextWrapping="Wrap"/>
        <Label x:Name="lblAbout" Content="www.barbez.eu" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,10,1" Foreground="#FF0066CC" Cursor="Hand" MouseDown="lblAbout_MouseDown" TouchDown="lblAbout_MouseDown" />
    </Grid>
</Window>
